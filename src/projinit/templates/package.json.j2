{
  "name": "{{ project_name }}",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "{{ description }}",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
{% if framework == "react" %}
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
{% elif framework == "vue" %}
    "vue": "^3.5.0"
{% else %}
    "lucide": "^0.400.0"
{% endif %}
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
{% if framework == "react" %}
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
{% elif framework == "vue" %}
    "@vitejs/plugin-vue": "^5.0.0",
{% endif %}
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vitest": "^2.0.0"
  }
